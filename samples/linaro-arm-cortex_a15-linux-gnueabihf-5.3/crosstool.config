CT_EXPERIMENTAL=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_WORK_DIR="/werkstatt/entwicklung/toolchains/ct-ng_linaro-build/.build"
CT_PARALLEL_JOBS=8
CT_LOG_EXTRA=y
CT_ARCH_CPU="cortex-a15"
CT_ARCH_FPU="neon-vfpv4"
CT_ARCH_FLOAT_HW=y
CT_ARCH_arm=y
CT_TOOLCHAIN_PKGVERSION="Linaro GCC 5.3 2015.12 - Cortex-A15"
CT_TARGET_VENDOR="cortex_a15"
CT_TARGET_ALIAS="arm-eabi"
CT_KERNEL_linux=y
CT_KERNEL_V_3_4=y
CT_BINUTILS_CUSTOM=y
CT_BINUTILS_CUSTOM_LOCATION="/werkstatt/entwicklung/toolchains/ct-ng_linaro-src/binutils-gdb"
CT_BINUTILS_CUSTOM_VERSION="2.6-branch"
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_CC_GCC_SHOW_LINARO=y
CT_CC_GCC_V_linaro_5_3_0=y
CT_CC_GCC_CORE_EXTRA_CONFIG_ARRAY="--with-float=hard"
CT_CC_GCC_EXTRA_CONFIG_ARRAY="--with-float=hard"
CT_CC_LANG_CXX=y
CT_DEBUG_gdb=y
# CT_GDB_CROSS_PYTHON is not set
