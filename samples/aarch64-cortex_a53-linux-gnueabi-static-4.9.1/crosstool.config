CT_EXPERIMENTAL=y
CT_DEBUG_CT=y
CT_DEBUG_CT_SAVE_STEPS=y
CT_DEBUG_INTERACTIVE=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_PARALLEL_JOBS=8
CT_LOG_EXTRA=y
CT_ARCH_CPU="cortex-a53"
CT_ARCH_64=y
CT_ARCH_arm=y
CT_STATIC_TOOLCHAIN=y
CT_TOOLCHAIN_PKGVERSION="Linaro GCC 4.9 - Cortex-A53"
CT_TARGET_VENDOR="cortex_a53"
CT_TARGET_ALIAS="arm-eabi"
CT_TOOLCHAIN_ENABLE_NLS=y
CT_KERNEL_linux=y
CT_KERNEL_V_3_10=y
CT_CC_BINUTILS_SHOW_LINARO=y
CT_BINUTILS_LINARO_V_2_25=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_CC_GLIBC_SHOW_LINARO=y
CT_LIBC_GLIBC_LINARO_V_2_20=y
CT_CC_GCC_SHOW_LINARO=y
CT_CC_GCC_V_linaro_4_9_1=y
# CT_CC_GCC_USE_GRAPHITE is not set
CT_CC_GCC_CORE_EXTRA_CONFIG_ARRAY="--enable-fix-cortex-a53-835769 --with-gnu-as --with-gnu-ld --disable-cloog-version-check --disable-isl-version-check --disable-ppl-version-check --enable-threads --disable-nls --disable-shared --disable-libitm --disable-libssp --disable-sjlj-exceptions --disable-libquadmath --enable-libgomp --disable-libsanitizer --enable-plugins"
CT_CC_GCC_EXTRA_CONFIG_ARRAY="--enable-fix-cortex-a53-835769 --with-gnu-as --with-gnu-ld --disable-cloog-version-check --disable-isl-version-check --disable-ppl-version-check --enable-threads --disable-nls --disable-shared --disable-libitm --disable-libssp --disable-sjlj-exceptions --disable-libquadmath --enable-libgomp --disable-libsanitizer --enable-plugins"
CT_CC_LANG_CXX=y
CT_DEBUG_gdb=y
CT_GDB_CROSS_EXTRA_CONFIG_ARRAY="--disable-expat --with-expat=no"
CT_GDB_NATIVE=y
CT_DEBUG_GDB_SHOW_LINARO=y
CT_GDB_V_linaro_7_8=y
CT_MPFR_V_3_1_2=y
CT_MPC_V_1_0_2=y
